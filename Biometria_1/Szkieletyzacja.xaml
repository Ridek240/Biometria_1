<Window x:Class="Biometria_1.Szkieletyzacja"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Biometria_1"
        mc:Ignorable="d"
        Title="Szkieletyzacja" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Image x:Name="ScelResult" Grid.Row="0" RenderOptions.BitmapScalingMode="NearestNeighbor"/>
        <Grid Grid.Row="1" VerticalAlignment="Center">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button Content="KMM" Grid.Column="0" Click="KMM"/>
            <Button Content="K3M" Grid.Column="1" Click="K3M"/>

        </Grid>
    </Grid>
</Window>
