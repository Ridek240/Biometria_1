<Window x:Class="Biometria_1.Klasification"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Biometria_1"
        mc:Ignorable="d"
        Title="Klasification" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Label Name="N"/>
            <Label Name="V1"/>
            <Label Name="V2"/>
        </StackPanel>
        <Label Grid.Row="1" Grid.Column="0">KNN</Label>
        <ScrollViewer Grid.Row="1" Grid.Column="1">
            <TextBlock x:Name="KNN"/>
        </ScrollViewer>
        <Label Grid.Row="2" Grid.Column="0">Naive Bayes</Label>
        <Label Grid.Row="2" Grid.Column="1" x:Name="NB"/>
        <Button Grid.Column="0" Grid.Row="3" Content="Losuj" Click="Button_Click" />
    </Grid>
</Window>
